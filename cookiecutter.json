{
    "target_project": "Name of the target repo being tested (e.g. 'workforce-one')",
    "environment_url": "URL of the environment to test (e.g. 'https://app.inv.tech/')",
    "project_name": "{{ cookiecutter.target_project }}_e2e",
    "project_slug": "{{ cookiecutter.project_name.lower().replace('_', '-').replace(' ', '-') }}",
    "project_short_description": "E2E testing suite for {{ cookiecutter.target_project }}",
